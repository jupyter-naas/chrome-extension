{
  "version": "2.0",
  "name": "Naas",
  "description": "NAAS is a all-in-one data science platform that enables everyone to analyze and strengthen their presence on social.",
  "permissions": ["storage", "cookies", "alarms", "notifications", "sidePanel"],
  "host_permissions": ["*://www.linkedin.com/", "*://naas.ai/"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "images/logo.png"
  },
  "side_panel": {
    "default_path": "popup/index.html"
  },
  "icons": {
    "128": "images/logo.png"
  },
  "manifest_version": 3
}
