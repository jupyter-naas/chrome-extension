{
    "version": "1.3",
    "name": "Naas",
    "description": "NAAS is a all-in-one data science platform that enables everyone to analyze and strengthen their presence on social.",
    "permissions": [
        "storage",
        "cookies",
        "alarms",
        "notifications"
    ],
    "host_permissions": [
        "*://www.linkedin.com/"
    ],
    "background": {
        "service_worker": "service-worker.js"
    },
    "action": {
        "default_popup": "/pages/popup.html",
        "default_icon": "/images/logo.png"
    },
    "icons": {
        "128": "/images/logo.png"
    },
    "manifest_version": 3
}